{
  "name": "@truestamp/truestamp-id",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/truestamp-id.cjs",
  "module": "./dist/truestamp-id.module.js",
  "umd:main": "./dist/truestamp-id.umd.js",
  "unpkg": "./dist/truestamp-id.umd.js",
  "jsdelivr": "./dist/truestamp-id.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/truestamp-id.cjs",
      "import": "./dist/truestamp-id.module.js",
      "node": "./dist/truestamp-id.cjs",
      "default": "./dist/truestamp-id.module.js"
    }
  },
  "license": "UNLICENSED",
  "amdName": "TruestampId",
  "version": "1.1.0",
  "author": "Truestamp <support@truestamp.com> (https://www.truestamp.com)",
  "description": "Truestamp Id serialize/deserialize library written in Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/truestamp/truestamp-id.git"
  },
  "bugs": {
    "url": "https://github.com/truestamp/truestamp-id/issues"
  },
  "homepage": "https://github.com/truestamp/truestamp-id",
  "scripts": {
    "clean": "rimraf dist",
    "package-check": "npx package-check",
    "build:web": "microbundle --target=web --tsconfig tsconfig.json --format es,umd,modern",
    "build:node": "microbundle --target=node --tsconfig tsconfig.json --format cjs",
    "build": "npm run package-check && npm run clean && npm run build:web && npm run build:node",
    "dev": "microbundle watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:format": "prettier --check \"{src,test}/**/*.ts\""
  },
  "bin": {
    "truestamp-id": "bin/truestamp-id.cjs"
  },
  "keywords": [
    "Truestamp",
    "JavaScript",
    "TypeScript",
    "Client",
    "CommonJS",
    "Node.js"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@noble/hashes": "^1.0.0",
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "^27.4.1",
    "@types/mocha": "^9.1.0",
    "jest": "^27.5.1",
    "microbundle": "^0.14.2",
    "mocha": "^9.2.2",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "superstruct": "^0.15.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "ulidx": "^0.3.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
