{
  "name": "@truestamp/truestamp-id",
  "amdName": "Truestamp",
  "version": "0.0.1",
  "author": "Truestamp <support@truestamp.com> (https://www.truestamp.com)",
  "description": "An ID serialize/deserialize utility written in Typescript that supports ES Modules, UMD, and CommonJS loaders and runs in Deno, Node.js, and modern browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/truestamp/truestamp-id.git"
  },
  "bugs": {
    "url": "https://github.com/truestamp/truestamp-id/issues"
  },
  "homepage": "https://github.com/truestamp/truestamp-id",
  "source": "src/index.ts",
  "main": "dist/truestamp-id.js",
  "module": "dist/truestamp-id.module.js",
  "esmodule": "dist/truestamp-id.modern.js",
  "umd:main": "dist/truestamp-id.umd.js",
  "unpkg": "dist/truestamp-id.umd.js",
  "jsdelivr": "dist/truestamp-id.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/truestamp-id.js",
      "import": "./dist/truestamp-id.module.js",
      "node": "./dist/truestamp-id.js",
      "default": "./dist/truestamp-id.module.js"
    }
  },
  "mangle": {
    "regex": "^_"
  },
  "scripts": {
    "clean": "rimraf dist",
    "package-check": "npx package-check",
    "build-web": "microbundle --target=web --tsconfig tsconfig.json --format es,umd,modern",
    "build-node": "microbundle --target=node --tsconfig tsconfig.json --format cjs",
    "build": "npm run package-check && npm run clean && npm run build-web && npm run build-node && npm run test",
    "dev": "microbundle watch --tsconfig tsconfig.json",
    "test": "jest --config jest.config.ts --detectOpenHandles",
    "serve": "npx serve --cors"
  },
  "keywords": [
    "Truestamp",
    "javascript",
    "typescript",
    "client",
    "universal",
    "ES Modules",
    "UMD",
    "CommonJS",
    "Deno",
    "Node.js",
    "Browser"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "protobufjs": "^6.11.2"
  },
  "devDependencies": {
    "@ctrl/ts-base32": "^2.0.1",
    "@skypack/package-check": "^0.2.2",
    "@types/jest": "^26.0.24",
    "@types/long": "^4.0.1",
    "@types/node": "^16.3.3",
    "ajv": "^8.6.2",
    "enc-utils": "^3.0.0",
    "fast-sha256": "^1.3.0",
    "fflate": "^0.7.1",
    "jest": "^27.0.6",
    "microbundle": "^0.13.3",
    "multihashes": "^4.0.2",
    "nock": "^13.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
