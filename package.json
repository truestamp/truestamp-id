{
  "name": "@truestamp/truestamp-id",
  "amdName": "TruestampId",
  "version": "0.0.1",
  "author": "Truestamp <support@truestamp.com> (https://www.truestamp.com)",
  "description": "Truestamp ID serialize/deserialize utility written in Typescript that supports Node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/truestamp/truestamp-id.git"
  },
  "bugs": {
    "url": "https://github.com/truestamp/truestamp-id/issues"
  },
  "homepage": "https://github.com/truestamp/truestamp-id",
  "source": "src/index.ts",
  "main": "dist/truestamp-id.js",
  "types": "dist/truestamp-id.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "package-check": "npx package-check",
    "build-node": "microbundle --target=node --tsconfig tsconfig.json --format cjs",
    "build": "npm run clean && npm run build-node && npm run test",
    "dev": "microbundle watch --tsconfig tsconfig.json",
    "test": "jest --config jest.config.ts --detectOpenHandles"
  },
  "keywords": [
    "Truestamp",
    "javascript",
    "typescript",
    "client",
    "CommonJS",
    "Node.js"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "ajv": "^8.6.2",
    "enc-utils": "^3.0.0",
    "fast-sha256": "^1.3.0",
    "fflate": "^0.7.1",
    "multihashes": "^4.0.2",
    "protobufjs": "^6.11.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@ctrl/ts-base32": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@types/long": "^4.0.1",
    "@types/node": "^16.3.3",
    "jest": "^27.0.6",
    "microbundle": "^0.13.3",
    "nock": "^13.1.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
